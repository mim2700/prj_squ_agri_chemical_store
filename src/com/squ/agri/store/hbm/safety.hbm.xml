<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="com.squ.agri.store.bo.Safety"
        table="M_SAFETY">

        <id name="safetyId"  column="S_ID">
           <generator class="assigned"/>
        </id>
        
        <property
            name="un_Number"
            column="S_un_NUMBER"/>

        <property
            name="other_number"
            column="S_NUMBER_OTHER"/>

        <property
            name="safetyDescription"
            column="S_DESCRIPTION"/> 
            
         <property
            name="safetyActive"
            column="S_ACTIVE"/> 

    </class>

</hibernate-mapping>