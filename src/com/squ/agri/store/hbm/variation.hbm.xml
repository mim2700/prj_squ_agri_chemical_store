<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="com.squ.agri.store.bo.Variation"
        table="M_VARIATION">

        <id name="variation_Id"  column="var_id">
           <generator class="assigned"/>
        </id>
        
        <property
            name="variation_Value"
            column="var_value"/>

        <property
            name="variation_Active"
            column="var_active"/>

    </class>

</hibernate-mapping>
